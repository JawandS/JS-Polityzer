    <script type="text/javascript">
        $(document).ready(function () {
            // Append required indicator          
            $('.f-val').each(function () {
                var p = $(this).data('val-controltovalidate');
                //console.log(p);
                $("label[for='" + p + "']").append('<span class=\"req\">*</span>');
            });

            // Remove redOutline class on focus
            $(".form-field").on('blur input', function () {//.focus(function () {
                $(this).removeClass("redOutline");
                $("label[for='" + this.id + "']").removeClass("req");
            });
        });


        function WebForm_OnSubmit() {
            if (typeof (ValidatorOnSubmit) == "function" && ValidatorOnSubmit() == false) {
                // Highlight fields

                $('#ValidationSummary1 li').each(function () {
                    var s = $('.form-unit').find('[data-val-errormessage="' + $(this).text() + '"]');
                    var p = $(s).data('val-controltovalidate');
                    $("label[for='" + p + "']").addClass("req");
                    $("#" + p).addClass("redOutline");
                });

                return false;
            }
            return true;
        }
    </script>


                    
