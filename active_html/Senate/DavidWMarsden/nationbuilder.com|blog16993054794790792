<script type="b1b7b78123f5c740b0a96dea-text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
<script type="b1b7b78123f5c740b0a96dea-text/javascript">
  $(document).ready(function () {
    const sliderOverlay = $('.content-hub--slider-overlay');
    const closeIcon = $("svg.close-dropdown");
    const slickSlider = $('.slick-slider');
    const dropdownListItems = $(".category-dropdown .dropdown-menu li");

    const closeOverlay = () => {
      sliderOverlay.hide();
      // $('body').css('overflow-y', 'auto');
      closeIcon.hide();
    }

    slickSlider.slick({
      infinite: false,
      appendArrows: $('.slick-controls'),
      slidesToShow: 2,
      slidesToScroll: 2,
      // focusOnChange: true,
      responsive: [
        {
          breakpoint: 768,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1,
            infinite: true,
          }
        }
      ],
      prevArrow: '<button type="button" name="previous posts" class="slider-prev"><svg width="36" height="36" viewBox="0 0 36 36" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M18 33C26.2843 33 33 26.2843 33 18C33 9.71573 26.2843 3 18 3C9.71573 3 3 9.71573 3 18C3 26.2843 9.71573 33 18 33Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/><path d="M18 12L12 18L18 24" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/><path d="M24 18H12" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg></button>',
      nextArrow: '<button type="button" name="next posts" class="slider-next"><svg width="36" height="36" viewBox="0 0 36 36" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M18 3C9.71573 3 3 9.71573 3 18C3 26.2843 9.71573 33 18 33C26.2843 33 33 26.2843 33 18C33 9.71573 26.2843 3 18 3Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/><path d="M18 24L24 18L18 12" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/><path d="M12 18L24 18" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg></button>'
    })

    dropdownListItems.on('click', function () {
      const categorySelected = $(this).data('value');

      // replace button text w/active dropdown selection
      $(this).parents(".category-dropdown-container").find('.btn').html($(this).text() + ' <svg width="23" height="21" viewBox="0 0 23 21" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M21.3548 9.63953L11.7151 19.4299L2.07531 9.63953" stroke="currentColor" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/></svg>');
           
      // show all list options
      dropdownListItems.show();

      // hide selected item from dropdown list
      $(this).hide();

      // hide section overlay
      sliderOverlay.hide();
      
      // filter category posts by selection
      slickSlider.slick('slickUnfilter');
      slickSlider.slick('slickFilter', `.${categorySelected}`);
      slickSlider.slick('slickGoTo', 0);

      // show View All button on mobile
      $('.category-view-all a').attr('href', `tags/${categorySelected}`);
    });

    // show section overlay when dropdown is open
    $(".category-dropdown .dropdown-toggle").on('click', function () {
      setTimeout(function () {
        if ($('.category-dropdown').hasClass('open')) {
          sliderOverlay.show();
          closeIcon.show();
        } else {
          closeOverlay();
        }
      }, 0);
    });

    // close section overlay 
    $(".category-dropdown .dropdown-menu").on('click', function () {
      closeOverlay();
    });
    sliderOverlay.on('click', function () {
      closeOverlay();
    });
    closeIcon.on('click', function () {
      closeOverlay();
    });

    // find most recent post
    let post_type = '';
    let most_recent_date = '';
    const posts = document.querySelectorAll('.post');
    posts.forEach((post) => {
      if (post.dataset.published_at > most_recent_date) {
        most_recent_date = post.dataset.published_at;
        post_type = post.dataset.tag_slug;
      }
    });
    // preselect category with most recent post on page load
    dropdownListItems.each((index, item) => {
      if ($(item).data('value') === post_type) {
        $(item).click();
      }
    })

    // select Product Features on hero button click
    $('#product-resources').on('click', function() {
      dropdownListItems[4].click(); // Product features
      ga('send', 'event', 'blog', 'Hero', 'Product Resources');
    });
    // select Enterprise Resources on hero button click
    $('#enterprise-resources').on('click', function() {
      dropdownListItems[5].click(); // Enterprise resources
      ga('send', 'event', 'blog', 'Hero', 'Enterprise Resources');
    });

    // Keyword Card Search
    // let cards = document.querySelectorAll('.post-filterable');
    let cards = $('.post-filterable');

    function noResultsCheck() {
      if (!$(".post-filterable").is(":visible")) {
        $('.no-results').show();
      } else {
        $('.no-results').hide();
      }
    }

    function search() {
      let search_query = $('#searchbox').val();
      for (var i = 0; i < cards.length; i++) {
        if(cards[i].innerText.toLowerCase().includes(search_query.toLowerCase())) {
          cards[i].classList.remove("is-hidden");
        } else {
          cards[i].classList.add("is-hidden");
        }
      }

      noResultsCheck();
    }

    // on filter tag click...
    $('.tag-container button').on('click', function () {
      const tagSelected = $(this).data('value');

      // show each post that was hidden by a filter
      $.each($(".post-filterable"), function() {
        if($(this).hasClass("filtered-hide")) {
          $(this).removeClass("filtered-hide");
        }
      });

      // toggle 'active' class on selected tag
      if ($(this).hasClass('active')) {
        // remove active class from tag selection
        $(this).removeClass('active');
      } else {
        // remove active class from previously selected tag selection
        $('.tag-container button').removeClass('active');
        // add active class to selected tag button
        $(this).addClass('active');

        // hide each post that is visible but not matching filter
        $.each($(".post-filterable"), function() {
          if($(this).is(":visible") && !$(this).hasClass(tagSelected)) {
            $(this).addClass("filtered-hide");
          }
        });
      }

      noResultsCheck();
    });
  
    let typingTimer;
    $('#searchbox').on('keyup', () => {
      clearTimeout(typingTimer);
      typingTimer = setTimeout(search, 500);
    })

    // open project modal on page load if query string matches
    const urlParams = new URLSearchParams(location.search);
    for (const [key, value] of urlParams) {
      // click filter tag based on query string
      $(`.tag-container button[data-value=${key}]`).click();
      // scroll down to search section
      if ($(`.tag-container button[data-value=${key}]`).length) {
        $('html, body').animate({
          'scrollTop':   $('#search').offset().top
        }, 2000);

        break;
      }
    }
  });

  /*
  This script targets elements with the data attribute "hidden". When the element enters the viewport, the class "visible" will be applied to it. Styling is handled via CSS.
  Example: <img data-hidden src="">
  */

  // IE doesn't support IntersectionObserver, so only run if it's available
  if (!!window.IntersectionObserver) {
    let observer = new IntersectionObserver((entries, observer) => {
      entries.forEach((entry) => {
        if (entry.isIntersecting) {
          entry.target.classList.add("visible");
        }
      });
    });

    document.querySelectorAll("[data-hidden]").forEach((hidden) => {
      observer.observe(hidden);
    });
  }
</script>
</div>

