    // Modal video
    var desktopIframe = $('#video-testimonial-529544944 iframe'),
      desktopFroogaloop = $f(desktopIframe[0]);

    desktopFroogaloop.addEvent('ready', function() {});

    $("#video-testimonial-529544944")
      .on('hidden.bs.modal', function() {
        desktopFroogaloop.api('pause');
      })
      .on('show.bs.modal', function() {
        desktopIframe.css("max-height", "none");
        desktopIframe.css("max-width", "none");
      })
      .on('shown.bs.modal', function() {
        desktopFroogaloop.api('play');
