<script src="https://3dna.nationbuilder.com/themes/24/58cbedc8cd0af55a03000000/0/attachments/14955746291698705763/mobile/countup.js" type="e4fd877c0f2087bb4c4950a6-text/javascript"></script>
<script type="e4fd877c0f2087bb4c4950a6-text/javascript">
  var emailsSent = new CountUp('count-emails-sent', 1000000000, 1700000000, 0, 1.5),
    volunteersMobilized = new CountUp('count-volunteers-mobilized', 300000, 386000, 0, 1.25),
    eventsHosted = new CountUp('count-events-hosted', 200000, 353000, 0, 1.25),
    contactsMade = new CountUp('count-contacts-made', 10000000, 23800000, 0, 1.25),
    dollarsRaised = new CountUp('count-dollars-raised', 300000000, 351600000, 0, 1),
    petitionsSigned = new CountUp('count-petitions-signed', 7000000, 9700000, 0, 1.5);

  inView('#count-emails-sent').once('enter', function() {
    emailsSent.start();
    volunteersMobilized.start();
    eventsHosted.start();
    contactsMade.start();
    dollarsRaised.start();
    petitionsSigned.start();
  });
</script>
