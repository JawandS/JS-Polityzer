<script type="e4fd877c0f2087bb4c4950a6-text/javascript">
  var wh = window.innerHeight,
    mid = wh / 2;

  var maxWidth = 1500;

  edgeImages( maxWidth );
  // fixNavBar();

  window.addEventListener( 'resize', function( event ) {
    wh = window.innerHeight;
    mid = wh / 2;

    edgeImages( maxWidth );
    // fixNavBar();
  }, true);

  // Stick left and right edge images to screen up until a certain width
  function edgeImages( max ) {
    document.querySelectorAll( '.image-rightedge' ).forEach( function( image ) {
      var position = image.getBoundingClientRect(),
        d = document.body.clientWidth;

      if ( d <= max ) {
        image.style.minWidth = ( d - position.left ) + 'px';
      }
    });

    document.querySelectorAll( '.image-leftedge' ).forEach( function( image ) {
      var position = image.getBoundingClientRect(),
        d = document.body.clientWidth;

      if ( d <= max ) {
        image.style.minWidth = position.right + 'px';
      }
    });
  }

  // Navbar top
  function fixNavBar() {
    var hero = document.getElementById( 'usecase-main' ),
      pos = hero.getBoundingClientRect(),
      nav = document.getElementById( 'slide-nav' );

    nav.style.top = pos.top + 'px';
  }

  // Scroll image changing
  document.addEventListener( 'scroll' , function( e ) {
    scrollImages( window.scrollY, mid );
  });

  function scrollImages( top, middle ) {
    var activeImage;

    document.querySelectorAll( '.nb-section-scroll__block--desktop' ).forEach( function( block ) {
      var position = block.getBoundingClientRect(),
        t = position.top,
        b = position.bottom;

      var parent = block.parentNode,
        firstChild = false,
        lastChild = false;

      var imageClass = block.dataset.image;

      if ( imageClass == parent.firstElementChild.dataset.image ) {
        firstChild = true;
      }
      else if ( imageClass == parent.lastElementChild.dataset.image ) {
        lastChild = true;
      }

      if ( firstChild && middle <= b ) {
        activeImage = imageClass;
      }
      else if ( lastChild && middle > t ) {
        activeImage = imageClass;
      }
      else if ( middle > t && middle <= b ) {
        activeImage = imageClass;
      }
    });

    document.querySelectorAll( '.nb-section-scroll__block__image--desktop' ).forEach( function( image ) {
      if ( image.classList.contains( activeImage ) ) {
        if ( !image.classList.contains( 'active' ) ) {
          image.classList.add( 'active' );
        }
      }
      else {
        image.classList.remove( 'active' );
      }
    });
  }
</script>
</div>

