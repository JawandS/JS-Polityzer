      var $shareWidgetList = $('.social-share-widget__menu-list');
      var shareWidgetClosed = true;
      function resizeShareWidget() {
        var $vw = $(window).width();
        if ($vw < 1130) {
          $shareWidget.remove('opened').addClass('closed');
          $shareWidgetList.attr('aria-hidden',true);
        } else {
          $shareWidget.removeClass('opened closed');
          $shareWidgetList.attr('aria-hidden',false);
        }
      }
      $('.social-share-widget__menu-toggle').on('click', function() {
        shareWidgetClosed = !shareWidgetClosed;
        $shareWidget.toggleClass('opened closed');
        $shareWidgetList.attr('aria-hidden',shareWidgetClosed);
      })
      $copyLink.on('mouseleave', function(e){
        $copyLink.removeClass('copied');
      });
      $copyLink.on('click',function(e) {
        e.preventDefault();
        copyText.select();
        copyText.setSelectionRange(0, 99999);
        document.execCommand("copy");
        $copyLink.addClass('copied');
        setTimeout(function() { $copyLink.removeClass('copied'); }, 3000);
      });
      $(window).resize(resizeShareWidget);
      resizeShareWidget();
    });
  </script>


<!-- /_social_share_widget.html -->


