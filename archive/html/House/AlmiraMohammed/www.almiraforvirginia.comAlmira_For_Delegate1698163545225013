        body_font_kit_id: window.Livewire.find('31phWXrVP0M39oFSKi1A').entangle('websiteData.body_font_kit_id'),
        body_font_name: window.Livewire.find('31phWXrVP0M39oFSKi1A').entangle('websiteData.body_font_family'),
        body_font_weight: window.Livewire.find('31phWXrVP0M39oFSKi1A').entangle('websiteData.body_font_weight'),
        body_font_size: window.Livewire.find('31phWXrVP0M39oFSKi1A').entangle('websiteData.body_font_size'),
        body_font_fallback: window.Livewire.find('31phWXrVP0M39oFSKi1A').entangle('websiteData.body_font_fallback'),
        getWebfontConfig(fontType, fontName, fontWeight, fontKitId) {
            if (fontType === 'google') {
                return {
                    google: {
                        families: [fontName + ':' + fontWeight + ',' + fontWeight + 'italic,700,700italic']
                    }
                };
            } else if (fontType === 'typekit') {
                return {
                    typekit: {
                        id: fontKitId
                    }
                };
            } else if (fontType === 'monotype') {
                return {
                    monotype: {
                        projectId: fontKitId,
                        version: Math.random().toString(36).replace(/[^a-z]+/g, '').substr(0, 5),
                        loadAllFonts: true
                    }
                };
            }
    
            return null;
    
        },
        loadFont() {
            try {
                let titleConfig = this.getWebfontConfig(this.title_font_type, this.title_font_name, this.title_font_weight, this.title_font_kit_id);
                let bodyConfig = this.getWebfontConfig(this.body_font_type, this.body_font_name, this.body_font_weight, this.body_font_kit_id);
    
                let finalConfig = null;
    
                if (!titleConfig && !bodyConfig) {
                    return;
                } else if (!titleConfig) {
                    finalConfig = bodyConfig;
                } else if (!bodyConfig) {
                    finalConfig = titleConfig;
                } else if (titleConfig.google && bodyConfig.google) {
                    finalConfig = {
                        classes: false,
                        google: {
                            families: [titleConfig.google.families[0], bodyConfig.google.families[0]]
                        }
                    };
                } else {
                    finalConfig = Object.assign({ classes: false }, titleConfig, bodyConfig);
                }
    
                
    
                window.WebFont.load(finalConfig);
            } catch (e) {
                console.log(e);
            }
        },
    }"
    x-init="$nextTick(() => {
        loadFont();
    });
    
    $watch('title_font_type', () => {
        if (title_font_name && title_font_weight && ((title_font_type !== 'monotype' && title_font_type !== 'typekit') || title_font_kit_id)) {
            loadFont();
        }
    });
    
    $watch('title_font_name', () => {
        if (title_font_name && title_font_weight && ((title_font_type !== 'monotype' && title_font_type !== 'typekit') || title_font_kit_id)) {
            loadFont();
        }
    });
    
    $watch('title_font_weight', () => {
        if (title_font_name && title_font_weight && ((title_font_type !== 'monotype' && title_font_type !== 'typekit') || title_font_kit_id)) {
            loadFont();
        }
    });
    
    $watch('title_font_kit_id', () => {
        if (title_font_name && title_font_weight && title_font_kit_id) {
            loadFont();
        }
    });
    
    $watch('body_font_type', () => {
        if (body_font_name && body_font_weight && ((body_font_type !== 'monotype' && body_font_type !== 'typekit') || body_font_kit_id)) {
            loadFont();
        }
    });
    
    $watch('body_font_name', () => {
        if (body_font_name && body_font_weight && ((body_font_type !== 'monotype' && body_font_type !== 'typekit') || body_font_kit_id)) {
            loadFont();
        }
    });
    
    $watch('body_font_weight', () => {
        if (body_font_name && body_font_weight && ((body_font_type !== 'monotype' && body_font_type !== 'typekit') || body_font_kit_id)) {
            loadFont();
        }
    });
    
    $watch('body_font_kit_id', () => {
        if (body_font_name && body_font_weight && body_font_kit_id) {
            loadFont();
        }
    });"
>
