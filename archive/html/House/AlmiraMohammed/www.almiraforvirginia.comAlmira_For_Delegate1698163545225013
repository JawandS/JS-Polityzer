                        <div class="ml-3 w-0 flex-1 pt-0.5">
                            <p x-text="message.title" class="font-medium text-base"
                                :class="{
                                    'text-green-800': message.type === 'success',
                                    'text-red-800': message.type === 'error' || message.type === 'fail',
                                    'text-blue-800': message.type === 'info',
                                    'text-yellow-800': message.type === 'warning',
                                }"></p>
                            <template x-if="message.message">
                                <p x-html="message.message" class="mt-1 text-sm"
                                :class="{
                                    'text-green-800/80': message.type === 'success',
                                    'text-red-800/80': message.type === 'error' || message.type === 'fail',
                                    'text-blue-800/80': message.type === 'info',
                                    'text-yellow-800/80': message.type === 'warning',
                                }"></p>
                            </template>
                        </div>
                        <div class="flex flex-shrink-0 ml-4">
                            <button @click="remove(message)" class="inline-flex transition duration-150 ease-in-out focus:outline-none"
                                :class="{
                                    'text-green-800 hover:text-green-900': message.type === 'success',
                                    'text-red-800 hover:text-red-900': message.type === 'error' || message.type === 'fail',
                                    'text-blue-800 hover:text-blue-900': message.type === 'info',
                                    'text-yellow-800 hover:text-yellow-900': message.type === 'warning',
                                }"
                            >
                                <svg class="w-5 h-5" viewBox="0 0 20 20" fill="currentColor">
                                    <path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd"></path>
                                </svg>
