    type: 'post',
    url: 'https://secure.ngpvan.com/v1/Forms/UaDehemY4EuTMi2qQxsz4g2',
    data: $('.ngp-signup-2').serialize(),
    dataType: 'json',
    success: function (s) {
      console.log(s);
      window.location.href='https://atoosareaser.com/';
    },
    error: function (f) {
      console.log(f);
      alert('Error submitting form.');
    }
  });

});

});  </script>		</div>
