		<!-- Segment: Form Data -->
		<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/4.1.1/crypto-js.min.js"></script>
		<script>
		$(document).ready(function() {
			const forms = document.querySelectorAll('form:not(.user_session_form):not(.account_form):not(.nation_signin_form)');
			console.log(`${forms.length} page form(s)`);
				
			
				
					
				
					
						
						
			
	
			forms.forEach((form) => {
				form.addEventListener('submit', (event) => {
					const inputFields = form.querySelectorAll('input:not([type="hidden"]):not([type="submit"])');
					const selectFields = form.querySelectorAll('select');
					let prependedPageTypeSlug = "signup_";

					let form_type = "";
					let event_type = "";
					let form_fields = {};

					if (prependedPageTypeSlug === "event_") {
						event_type = "rsvp";
						prependedPageTypeSlug = "event_rsvp_";
					} else if (prependedPageTypeSlug === "survey_") {
						event_type = "response";
						prependedPageTypeSlug = "survey_response_";
					}

					for (const field of inputFields) {
						// TODO: track selected radio button from group of radios
						// if (field.type == 'radio' && !field.checked) {
						// 	break;
						// } else if (field.type == 'radio') {
						// 	field.value = field.labels[0].textContent;
						// }

						if (field.type == 'checkbox') {
							field.value = field.checked;
						}

						if (field.id.startsWith(`${prependedPageTypeSlug}custom_values_`)) {
							field.id = field.id.replace(`${prependedPageTypeSlug}custom_values_`, "").replace("_custom", "");
						} else if (field.id.startsWith(prependedPageTypeSlug)) {
							field.id = field.id.replace(prependedPageTypeSlug, "");
						}
						form_fields[field.id] = field.value;
					}

					for (const field of selectFields) {
						if (field.id.startsWith(`${prependedPageTypeSlug}custom_values_`)) {
							field.id = field.id.replace(`${prependedPageTypeSlug}custom_values_`, "").replace("_custom", "");
						} else if (field.id.startsWith(prependedPageTypeSlug)) {
							field.id = field.id.replace(prependedPageTypeSlug, "");
						}
						form_fields[field.id] = field.selectedOptions[0].value === "" ? "" : field.selectedOptions[0].text;
					}

					
						form_type = "segment-sales-form".replace("segment-", "").replace("-form", "");
					

					form_type = form_type.length > 0 ? form_type : event_type;

					form_fields['submit_button_text'] = form.querySelector('input[type=submit]').value;

					if (form_fields.email) {
						const segment_user_id = CryptoJS.MD5(form_fields.email).toString();

						analytics.identify(segment_user_id, {
							email: form_fields.email,
							first_name: form_fields.first_name,
							last_name: form_fields.last_name
						});
						analytics.track('Form Submitted', {
							form_type: form_type,
							user_id: segment_user_id,
							page_slug: "enterprise",
							page_type: "signup",
							page_title: "Software to scale your organization | NationBuilder",
							email: form_fields.email,
							form_fields: form_fields
						});
					}
				});
			});
		});
		</script>
		<!-- End Segment: Form Data -->
		
		<!-- Segment: Intercom -->
		<script>
			!(function() {
				if (!String.prototype.includes) {
					String.prototype.includes = function(search, start) {
						if (search instanceof RegExp) {
							throw TypeError('first argument must not be a RegExp');
						}
						if (start === undefined) { start = 0; }
						return this.indexOf(search, start) !== -1;
					};
				}
				analytics.ready(function() {
					var prevRole = localStorage.getItem("ajs_user_traits")
						? JSON.parse(localStorage.getItem("ajs_user_traits")).role
						: null;
					var role = "visitor";
					if (prevRole) {
						role = prevRole;
					}
					function fireIdentify(id) {
						analytics.identify({
							role: role
						},	{
							context: {
								externalIds: [
									{
										id: id,
										type: "intercom_visitor_id",
										collection: "users",
										encoding: "none"
									}
								]
							}
						}, {
							integrations: {
								'Intercom': false
							}
						});
					}
					var visitorId = null;
					var visitorLoopIteration = 0;
					var isUser = localStorage.getItem("ajs_user_id");
					if (!isUser.includes('null')) {
						return;
					}
					var visitorIdInterval = setInterval(function() {
						visitorLoopIteration++;
						visitorId = window.Intercom("getVisitorId");
						if (visitorLoopIteration > 600) {
							clearInterval(visitorIdInterval);
						}
						if (visitorId) {
							fireIdentify(visitorId);
							clearInterval(visitorIdInterval);
						}
					}, 100);
				});
			})();
		</script>
		<!-- End Segment: Intercom  -->
		
