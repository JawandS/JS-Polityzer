<script src="https://player.vimeo.com/api/player.js"></script>
<script type="text/javascript">
// Modal video container
$(document).ready(function(){
  var headerContainer = $('#header_container');
  var videoContainer = $('#modal-video');
  var viewportWidth = $(window).width();
  var playButton = $('#video-play-button');
  var stopButton = $('#modal-video-close-button');
  var videoContent = $('#video-content');
  var videoPlayer = new Vimeo.Player(videoContent[0]);
  var videoURL = window.location.protocol + videoContent.attr('src').split('?');
  var isIOS = navigator.userAgent.match(/(iPod|iPhone|iPad)/);
  var isIPhone = navigator.userAgent.match(/(iPod|iPhone)/);
  var isIPad = navigator.userAgent.match(/(iPad)/);

  videoPlayer.on('finish', stopVideo);

  if (isIOS) {
    var videoWrapper = $('#video-container');
    videoWrapper.empty();
    videoWrapper.css('height','auto');
    videoContent.attr({'width':'100%','height':'100%'}).detach().appendTo(videoWrapper);
  }

  function playVideo() {
    videoContainer.css('z-index',99999).fadeTo(800,1,function(){
      videoPlayer.play().catch(function(error) {
        console.error('error playing the video:', error.name);
      });
    });
    $(".nav-trial").fadeOut(800);
  }

  function stopVideo() {
    if (isIOS) {
      videoContainer.fadeTo(800,0,function(){videoContainer.css('z-index',-1);});
    } else {
      videoPlayer.pause().catch(function(error) {
        console.error('error pausing the video:', error.name);
      });
      videoContainer.fadeTo(800,0,function(){videoContainer.css('z-index',-1);});
    }
    $(".nav-trial").fadeIn(800);
  }

  $(document).on('click', '#video-play-button, .play-include', function(){
    playVideo();
    headerContainer.fadeOut(800,function(){$(this).addClass('has-video')});
  });

  $(document).on('click', '#modal-video-close-button', function(){
    stopVideo();
    headerContainer.removeClass('has-video').fadeIn(800);
  });

});
</script>

<!-- /_network_video.html -->

