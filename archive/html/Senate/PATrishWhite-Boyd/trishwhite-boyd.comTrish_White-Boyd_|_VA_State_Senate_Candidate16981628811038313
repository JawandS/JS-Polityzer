<script>
function scrollNav() {
  jQuery('.navbar-nav a').click(function(){
    jQuery(".active").removeClass("active");     
    jQuery(this).addClass("active");
    
    jQuery('html, body').stop().animate({
      scrollTop: jQuery(jQuery(this).attr('href')).offset().top -130
    }, 700);
    return false;
  });
}
scrollNav(); 
</script>
