ESC"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"> <path d="M16.5 15.5L12 20M12 20L7.5 15.5M12 20V4" stroke="#B7E4CF" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round"/> </svg></div><div class="option hide" value="orderby=date&order=ASC" style="display:none;"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" style="transform:rotate(180deg)"> <path d="M16.5 15.5L12 20M12 20L7.5 15.5M12 20V4" stroke="#B7E4CF" stroke-width="1.8" stroke-linecap="round" stroke-linejoin="round"/> </svg></div></div></div></div></div><div class="et_pb_module et_pb_code et_pb_code_1_tb_body"><div class="et_pb_code_inner"><div id="state_posts_wrap"></div></div></div><div class="et_pb_module et_pb_code et_pb_code_2_tb_body"><div class="et_pb_code_inner"><script> jQuery(function($){ 
function querystates(first = false) {
 var queryString = window.location.search;
 //console.log(queryString);
 var urlParams = new URLSearchParams(queryString); 
 var page1 = urlParams.get('pages');
 //console.log(page1);
 //console.log('PAGES' + page1);
 var datacat = $( "select#categories.statestag option:selected" ).val();
 //   var datastate = $( "select#states.statestag option:selected" ).val();
 // var sortby = $( ".sortby option:selected" ).val();
 var sortby = $( ".sortblock > .option.active" ).attr("value"); 
 // 	console.log("function:");
 //	console.log("sort -"+ sortby + "");
 //	console.log("tag -"+ datacat + "");
 // console.log("state -"+ datastate + "");
 var postt = '' ;
 var datatagp = '' ;
 var datastatep = "";
 var datapage='';
 if (datacat > 0) {
 var datatagp = '&category='+ datacat ;
 }
 //    if (datastate > 0) {
 //      var datastatep = '&state='+ datastate ;
 //    }
 if (page1 > 0 && first) {
 var datapage = '&page='+ page1 ;
 }
 var datastream = '' + sortby + datatagp + datapage +  '&action=newsfeedload';
 //	console.log(datastream);
 $.ajax({
 url:'https://millerpitts4va.com/wp-admin/admin-ajax.php',
 data : datastream, // form data
 dataType : 'json', // this data type allows us to receive objects from the server
 type : 'POST',
 success : function( data ){
 // console.log(data);
 // when filter applied:
 // set the current page to 1
 state_loadmore_params.current_page = 1;
 //console.log(data);
 // set the new query parameters
 state_loadmore_params.posts = data.posts; 
 // set the new max page parameter
 state_loadmore_params.max_page = data.max_page; 
 // change the button label back
 $('#state_filters').find('button').text('Apply filter'); 
 // insert the posts to the container
 $('#state_posts_wrap').html(data.content); 
 // hide load more button, if there are not enough posts for the second page
 if ( data.max_page < 2  || (first && page1 && data.max_page <= page1)) {
 $('#state_loadmore').hide();
 } else {
 $('#state_loadmore').show();
 } 
 }
 });
 //	console.log("function end");
 // do not submit the form
 return false;
 }
querystates(true);
 $('select.statestag').change(function(){
 querystates();
 }); 
 $('.sortby').change(function(){
 querystates();
 }); 
 jQuery('.sortblock').click( function() {
 if (jQuery('.sortblock .option.hide').hasClass("active")) {
 jQuery('.sortblock .option.show').addClass('active').css('display', 'flex');
 jQuery('.sortblock .option.hide').removeClass('active').hide();
 } else {
 jQuery('.sortblock .option.show').removeClass('active').hide();
 jQuery('.sortblock .option.hide').addClass('active').css('display', 'flex');
 }
 querystates();
 });
 $('body').on('click', '#state_loadmore', function(){
 state_loadmore_params.current_page =  $('.page-numbers.current').data("val");
 $.ajax({
 url : state_loadmore_params.ajaxurl, // AJAX handler
 data : {
 'action': 'loadmore', // the parameter for admin-ajax.php
 'query': state_loadmore_params.posts, // loop parameters passed by wp_localize_script()
 'page' : state_loadmore_params.current_page, // current page
 'first_page' : state_loadmore_params.first_page,
 'type' : 'loadmore'
 },
 type : 'POST',
 beforeSend : function ( xhr ) {
 $('#state_loadmore').text('Loading...'); // some type of preloader
 },
 success : function( data ){
 var data = JSON.parse(data);
 $( "#pagination .nav-links" ).html(data.html_paginator);
 $( "#pagination" ).before(data.content);
 if ( (state_loadmore_params.current_page + 1) >= data.max_page) {
 $('#state_loadmore').hide();
 } else {
 $('#state_loadmore').show();
 }
 $('#state_loadmore').text('More posts'); 
 state_loadmore_params.current_page++;  }
 });
 return false;
 }); 
 $('body').on('click', '.page-numbers', function(){
 state_loadmore_params.current_page = $(this).data("val");
 $.ajax({
 url : state_loadmore_params.ajaxurl, // AJAX handler
 data : {
 'action': 'loadmore', // the parameter for admin-ajax.php
 'query': state_loadmore_params.posts, // loop parameters passed by wp_localize_script()
 'page' : state_loadmore_params.current_page, // current page
 'first_page' : state_loadmore_params.first_page,
 'type' : 'paginator'
 },
 type : 'POST',
 beforeSend : function ( xhr ) {
 //$('#state_loadmore').text('Loading...'); // some type of preloader
 },
 success : function( data ){
 var data = JSON.parse(data);
 //console.log(data);
 $('#state_posts_wrap article').remove();
 $( "#pagination .nav-links" ).html(data.html_paginator);
 $( "#pagination" ).before(data.content);
 if ( (state_loadmore_params.current_page) >= data.max_page ) {
 $('#state_loadmore').hide();
 } else {
 $('#state_loadmore').show();
 }
 }
 });
 return false;
 }); 
}); </script></div></div></div></div></div></div></div></div><footer class="et-l et-l--footer"><div class="et_builder_inner_content et_pb_gutters3"><div class="et_pb_section et_pb_section_0_tb_footer et_pb_with_background et_section_regular" ><div class="et_pb_row et_pb_row_0_tb_footer et_pb_equal_columns"><div class="et_pb_column et_pb_column_1_2 et_pb_column_0_tb_footer  et_pb_css_mix_blend_mode_passthrough"><div class="et_pb_module et_pb_text et_pb_text_0_tb_footer  et_pb_text_align_left et_pb_text_align_center-tablet et_pb_bg_layout_light"><div class="et_pb_text_inner"><p>Newsletter</p></div></div><div class="et_pb_module et_pb_text et_pb_text_1_tb_footer  et_pb_text_align_left et_pb_text_align_center-tablet et_pb_bg_layout_light"><div class="et_pb_text_inner"><p>Get regular updates</p></div></div></div><div class="et_pb_column et_pb_column_1_2 et_pb_column_1_tb_footer  et_pb_css_mix_blend_mode_passthrough et-last-child"><div class="et_pb_module et_pb_code et_pb_code_0_tb_footer"><div class="et_pb_code_inner"><div class="forminator-ui forminator-custom-form forminator-custom-form-601 forminator-design--default  forminator_ajax" data-forminator-render="0" data-form="forminator-module-601" data-uid="654961c5dfbd0"><br/></div><form
