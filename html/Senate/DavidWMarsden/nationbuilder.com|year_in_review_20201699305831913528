            $(document).ready(function() {

              window.addEventListener('scroll', event => {
                if($('#mapanimation').visible(true)){
                  anmap.play();
                } else {
                  anmap.stop();
                }
                if($('#mapanimationdesktop').visible(true)){
                  anmapdesktop.play();
                } else {
                  anmapdesktop.stop();
                }
              });

              const anmap = lottie.loadAnimation({
                container: document.getElementById('mapanimation'), // the dom element that will contain the animation
                renderer: 'svg',
                loop: true,
                autoplay: false,
                path: 'https://assets.nationbuilder.com/themes/58cbedc8cd0af55a03000000/attachments/original/1640127297/map_mobile.json?1640127297' // the path to the animation json
              });

              const anmapdesktop = lottie.loadAnimation({
                container: document.getElementById('mapanimationdesktop'), // the dom element that will contain the animation
                renderer: 'svg',
                loop: true,
                autoplay: false,
