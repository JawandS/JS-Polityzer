<script type="708af7f8c3fb882b736a3e59-text/javascript">

            $(document).ready(function() {

              window.addEventListener('scroll', event => {
                if($('#bakendanimation').visible(true)){
                  bakendanimation.play();
                } else {
                  bakendanimation.stop();
                }
                if($('#bakendanimationdesktop').visible(true)){
                  bakendanimationdesktop.play();
                } else {
                  bakendanimationdesktop.stop();
                }
              });

              const bakendanimation = lottie.loadAnimation({
                container: document.getElementById('bakendanimation'), // the dom element that will contain the animation
                renderer: 'svg',
                loop: true,
                autoplay: false,
                path: 'https://assets.nationbuilder.com/themes/58cbedc8cd0af55a03000000/attachments/original/1640127466/nb_2020_backend_animation.json?1640127466' // the path to the animation json
              });

              const bakendanimationdesktop = lottie.loadAnimation({
                container: document.getElementById('bakendanimationdesktop'), // the dom element that will contain the animation
                renderer: 'svg',
                loop: true,
                autoplay: false,
