<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://unpkg.com/list.js@1.5.0/dist/list.min.js"></script>
<script src="https://3dna.nationbuilder.com/themes/95/632cbac54445ea6c7a0a1d71/0/attachments/16638759321698869699/mobile/smoothscroll.js" type="text/javascript"></script>
<script>
  (function () {
    const partnersDirectory = document.querySelector('.partners-directory');
    const partnersDirectoryForm = document.querySelector('.partners-directory-filters form');
    const partnersDirectoryContainer = document.querySelector('.partners-directory-container');
    const partnersDirectoryNoResults = document.querySelector('.partners-directory-no-results');
    const partnersDirectoryList = document.querySelector('.partners-directory-list');
    const partnersDirectoryPagination = document.querySelector('.partners-directory-pagination');
    const partnersDirectoryPaginationWrapper = document.querySelector('.partners-directory-pagination-wrapper');
    // Filters
    let industryFilter = $('input[name="industry"]:checked');
    let affiliationFilter = $('input[name="affiliation"]:checked');
    let regionFilter = $('input[name="region"]:checked');
    let partnerFilter = $('input[name="partner"]').is(':checked');
    let certificationFilter;
    // List.js setup
    let partnersList;
    const options = {
      valueNames: [{ data: ['name', 'description', 'industries', 'regions', 'affiliations', 'certifications', 'partner_match', 'sort_priority'] }],
      page: 30,
      pagination: true,
      searchColumns: ['name', 'description']
    }
    // Total number of pages to loop over
    const totalPages = Math.ceil(partnersDirectoryContainer.getAttribute("data-total-count") / 30);
    let dataCountdown = totalPages;

    // Watch for filter changes
    $('.custom-control-input').on('change', function() {
      industryFilter = $('input[name="industry"]:checked');
      affiliationFilter = $('input[name="affiliation"]:checked');
      regionFilter = $('input[name="region"]:checked');
      partnerFilter = $('input[name="partner"]').is(':checked');

      certificationFilter = new Array;
      $.each($('input[name="certification"]:checked'), function() {
        certificationFilter.push($(this).val());
      });

      // Get currently selected values of each filter
      const industryFilterValue = industryFilter.val();
      const regionFilterValue = regionFilter.val();
      const affiliationFilterValue = affiliationFilter.val();
      const partnerFilterValue = partnerFilter;
      const certificationFilterValues = certificationFilter;

      partnersList.filter(function (item) {
        let industries = item.values().industries || "";
        let affiliations = item.values().affiliations || "";
        let regions = item.values().regions || "";
        let partner_match = item.values().partner_match || false;
        let certifications = item.values().certifications || "";

        let certificationsMatch = false;

        if (certifications) {
          certifications = certifications.split(' ');
          for (let i = 0; i < certificationFilterValues.length; i++) {
            certificationsMatch = certifications.includes(certificationFilterValues[i]);
          }
        }

        if (certificationFilterValues.length < 1) {
          certificationsMatch = true;
        }

        return (
          (industryFilterValue === "any" || 
          industries.includes(industryFilterValue)) &&
          (regionFilterValue === "any" || 
          regions.includes(regionFilterValue)) &&
          (affiliationFilterValue === "any" || 
          affiliations.includes(affiliationFilterValue)) &&
          (!partnerFilterValue || partner_match === "true") &&
          (certificationsMatch));
      });

      setPaginationAndNoResults();
    });

    // After pagination click, scroll to the top of the page
    document.addEventListener('click', function(event) {
      if (event.target.classList.contains('page') || event.target.classList.contains('btn')) {
        partnersDirectory.scrollIntoView({
          behavior: 'smooth'
        });
        setButtonState($('li.active')[0].innerText); // based on current page

        // set attribute with partner count
        const partnerCount = document.getElementById('directory-listings').childElementCount;
        partnersDirectoryList.setAttribute('data-page-count', partnerCount);
      }
    });

    // disable/enable pagination buttons
    function setButtonState(currentPage) {
      const lastPage = Math.ceil(partnersList.matchingItems.length / 30) + '';

      // enable buttons
      $('button.prev').attr('disabled', false);
      $('button.next').attr('disabled', false);

      // disable button if first or last page
      if (currentPage.localeCompare('1') === 0) {
        $('button.prev').attr('disabled', true);
      } else if (currentPage.localeCompare(lastPage) === 0) {
        $('button.next').attr('disabled', true);
      }
    }

    function appendPageData(items) {
      Array.prototype.slice.call(items).forEach(function (item) {
        partnersDirectoryList.append(item);
      });
    }

    function fetchPageData(pageNum) {
      axios.get("https://www.nationbuilder.partners/partner_directory", {
        params: {
          page: pageNum,
        },
      })
        .catch(function(error) {
          console.error(`${error.name}: ${error.message}`, error);
        })
        .then(function(response) {
          const dom = stringToHTML(response.data);
          appendPageData(dom.children);
          dataCountdown--;

          if (dataCountdown === 0) {
            initListJs();
          }
        })
        .catch(function(error) {
          console.error(error);
        });
    }

    // Hide/show pagination and no results elements based on filtered results
    function setPaginationAndNoResults() {
      document.querySelector('.fuzzy-search').placeholder =`Search ${partnersList.matchingItems.length} partners`;

      if (partnersList.matchingItems.length <= 30) {
        partnersDirectoryPaginationWrapper.classList.add('d-none');
        $('.partners-directory-results button').addClass('d-none');
      } else {
        partnersDirectoryPaginationWrapper.classList.remove('d-none');
        $('.partners-directory-results button').removeClass('d-none');

        // active/current page number
        setButtonState($('li.active')[0].innerText);

        // pagination button listener
        $('.partners-directory-results button').on('click', (e) => {
          const currentPage = $('li.active')[0];
          const currentPageText = $('li.active')[0].innerText;
          const nextPage = currentPage.nextSibling;
          const nextPageText = nextPage ? nextPage.innerText : '';
          const prevPage = currentPage.previousSibling;
          const prevPageText = prevPage ? prevPage.innerText : '';
          const clickedButtonText = e.target.innerText;

          if (clickedButtonText === 'Next' && nextPage) {
            nextPage.click();
            setButtonState(nextPageText);
          } else if (clickedButtonText === 'Prev' && prevPage) {
            prevPage.click();
            setButtonState(prevPageText);
          }
        });
      }

      if (partnersList.matchingItems.length == 0) {
        partnersDirectoryNoResults.classList.remove('d-none');
      } else {
        partnersDirectoryNoResults.classList.add('d-none');
      }
    }

    // Loop over each page and fetch the page markup
    function fetchPages() {
      for (let i = 1; i <= totalPages; i++) {
        fetchPageData(i);
      }
    }

    // Kick off the requests to fetch the partners by page
    fetchPages();

    function initListJs() {
      partnersList = new List('list-js', options);
      setPaginationAndNoResults();
      $('#partner-search').on('keyup', function() {
        const searchString = $(this).val();
        partnersList.search(searchString);
      });
      partnersList.on('searchComplete', setPaginationAndNoResults);
      // sort partners by custom field, "partner_directory_priority" (asc), and then by "name" (desc)
      partnersList.sort('', {order: "", sortFunction: sortList(partnersList, "sort_priority", "asc", "name", "desc")});
      // check for query string filters
      checkQueryString();
      // display results
      partnersDirectoryList.classList.add('d-flex');
      partnersDirectoryContainer.classList.remove('loading');
    }

    function stringToHTML(str) {
      const dom = document.createElement("div");
      dom.innerHTML = str;
      return dom;
    }

    if (window.NodeList && !NodeList.prototype.forEach) {
      NodeList.prototype.forEach = function (callback, thisArg) {
        thisArg = thisArg || window;
        for (let i = 0; i < this.length; i++) {
          callback.call(thisArg, this[i], i, this);
        }
      };
    }

    // Sort List.js list by multiple fields
    function sortList (list, first_field, first_field_order, second_field, second_field_order) {
      // Call List.js sort method
      list.sort('', {order: '',
          sortFunction:
              function(a, b) {
                return sortField(a, b, first_field, first_field_order)
                    || sortField(a, b, second_field, second_field_order);
              }
          }
      );
    }

    // Sort List.js list items by specified field
    function sortField(a, b, field, sort_order) {
      a.values()[field] = a.values()[field] || '';
      b.values()[field] = b.values()[field] || '';

      // convert sort_priority string to integer
      if (field === "sort_priority") {
        a.values()[field] = Math.round(a.values()[field]);
        b.values()[field] = Math.round(b.values()[field]);
      }

      if (Number.isInteger(a.values()[field]) && Number.isInteger(b.values()[field])) {
        // sort integers
        return sort_order === "asc"
            ? a.values()[field] - b.values()[field]
            : b.values()[field] - a.values()[field];
      } else {
        // sort strings
        return sort_order === "asc"
            ? b.values()[field].localeCompare(a.values()[field])
            : a.values()[field].localeCompare(b.values()[field]);
      }
    }

    // filter list based on query string
    function checkQueryString() {
      const urlParams = new URLSearchParams(location.search);
      for (const [key, value] of urlParams) {
        // if filter has not already been selected, select it
        // filter by nb partner match
        if (key === "partner" && value === "true") {
          $(`input[name=${key}]`).click();
        }
        // filter by all certifications
        else if (key === "certification" && value === "true") {
          $(`#${key}`).click();
          $(`input[name=${key}][value=architect]`).click();
          $(`input[name=${key}][value=developer]`).click();
          $(`input[name=${key}][value=expert]`).click();
        }
        // filter by industry, affiliation, region, certification
        else if (!$(`input[name=${key}][value=${value}]`).is(':checked')) {
          $(`#${key}`).click();
          $(`input[name=${key}][value=${value}]`).click();
        }
      }
    }

    // reposition pagination based on # of partners shown on the page
    function positionPagination() {
      if (window.innerWidth >= 1200) {
        // check height of filters column
        const leftColumnHeight = $('.partners-directory-search').height() + $('.partners-directory-search ~ form').height();
        // check height of listings column
        const rightColumnHeight = $('#directory-listings').height();
        // compare heights
        if (leftColumnHeight >= rightColumnHeight) {
          partnersDirectoryPaginationWrapper.classList.add('short-page');
          $('.partners-directory-results').css('height', leftColumnHeight + 120);
        } else {
          partnersDirectoryPaginationWrapper.classList.remove('short-page');
          $('.partners-directory-results').css('height', 'auto');
        }
      } else {
        partnersDirectoryPaginationWrapper.classList.remove('short-page');
        $('.partners-directory-results').css('height', 'auto');
      }
    }

    // Select the node that will be observed for mutations
    let targetNode = document.getElementById('directory-listings');
    const config = { attributes: true };

    const callback = (mutationList, observer) => {
      for (const mutation of mutationList) {
        if (window.innerWidth >= 1200) {
          positionPagination();
        }
      }
    }

    // Start observing the target node for configured mutations
    const observer = new MutationObserver(callback);
    observer.observe(targetNode, config);

    // reposition pagination on resize
    window.addEventListener('resize', positionPagination);
  })();
</script>

</main>

